            </for>
            <output expression="&quot; Jadi total penegluaran nya sebesar &quot; &amp;total&amp; &quot; dan harga hari ke 1 adalah &quot; &amp;awal" newline="True"/>
            <assign variable="akhir" expression="60000"/>
            <assign variable="akhir" expression="70000"/>
            <assign variable="akhir" expression="80000"/>
            <assign variable="akhir" expression="90000"/>
        </body>

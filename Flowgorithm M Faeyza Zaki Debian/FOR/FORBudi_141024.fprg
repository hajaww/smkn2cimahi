    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="awal, akhir, perhari, hari, total" type="Integer" array="False" size=""/>
            <assign variable="awal" expression="50000"/>
            <assign variable="perhari" expression="10000"/>
            <assign variable="akhir" expression="awal"/>
